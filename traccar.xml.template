    <entry key='geolocation.url'>https://us1.unwiredlabs.com/v2/process</entry>
    <entry key='geolocation.key'>pk.d234dfeff96d1dd6704fd6b22c600ca6</entry>
    <entry key='geolocation.processInvalidPositions'>true</entry-->

    <entry key='geolocation.enable'>true</entry>
    <entry key='geolocation.requireWifi'>false</entry>
    <entry key='geolocation.type'>google</entry>
    <entry key='geolocation.key'>AIzaSyB4qeQAk5fKGTq1gstFhsoDrC8WVatQDks</entry>

    <entry key='logger.console'>true</entry>
    <entry key='logger.level'>info</entry>
    <entry key='logger.fullStackTraces'>true</entry>
</properties>
